SELECT * FROM transaction



SELECT c.name, ROUND(SUM(t.amount),2),COUNT (*)
FROM coins c join 
transaction t  
on c.id = t.coin_id 
WHERE EXTRACT (YEAR FROM t.dt::date)= 2023
group by c.name


