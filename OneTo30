CREATE TABLE product (
    id INT NOT NULL PRIMARY KEY,
    name VARCHAR(255),
    price DECIMAL(6, 2),
    in_stock SMALLINT
);

-- Create wishlists table
CREATE TABLE wishlists (
    product_id INT,
    customer_email VARCHAR(255),
    FOREIGN KEY (product_id) REFERENCES product(id)
);

-- ===================================
-- INSERT DATA
-- ===================================

-- Insert data into products table
