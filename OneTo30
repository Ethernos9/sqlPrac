INSERT INTO EMPLOYEE (NAME, PHONE, AGE) VALUES ('Sam', '1000040000', 30);
INSERT INTO EMPLOYEE (NAME, PHONE, AGE) VALUES ('Chris', '1000012000', 34);
INSERT INTO EMPLOYEE (NAME, PHONE, AGE) VALUES ('Chris', '1000012000', 34);


SELECT DISTINCT NAME
FROM EMPLOYEE 
group by name , phone , age
having count(*)>1




