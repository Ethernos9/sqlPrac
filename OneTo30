SELECT 
    a.iban,
    ROUND(SUM(i.amount), 2) AS total_income,
    '20%' AS tax_rate,
    ROUND(SUM(i.amount) * 0.20, 2) AS calculated_tax
FROM accountss a
JOIN incomess i 
    ON a.id = i.account_id
WHERE EXTRACT(YEAR FROM i.dt::date) = 2023
GROUP BY a.iban
ORDER BY a.iban;








