CREATE TABLE userss (
    id INT PRIMARY KEY,
    email VARCHAR(255) NOT NULL
);

-- Create transactions table
CREATE TABLE transactions (
    user_id INT REFERENCES userss(id),
    dt VARCHAR(19) NOT NULL,
    amount DECIMAL(5,2) NOT NULL
);



